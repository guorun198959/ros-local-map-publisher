    <param name="use_sim_time" value="true" />
    <rosparam file="$(find global_path_planner)/config/config.yaml" />
    <node name="local_costmap" pkg="global_path_planner" type="local_costmap" output="screen" />
    <node type="rviz" name="rviz" pkg="rviz" args="-d $(find global_path_planner)/rviz/rviz_config.rviz" />
</launch>